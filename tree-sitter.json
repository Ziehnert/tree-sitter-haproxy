{
    "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
    "grammars": [
        {
            "name": "haproxy",
            "camelcase": "HAProxy",
            "title": "HAProxy",
            "scope": "source.haproxy",
            "file-types": ["haproxy"],
            "injection-regex": "^haproxy$",
            "class-name": "TreeSitterHaproxy"
        }
    ],
    "metadata": {
        "version": "0.1.0",
        "license": "MIT",
        "description": "HAProxy tree-sitter for zed",
        "authors": [
            {
                "name": "Simon Ziehnert",
                "email": "simon@snowbyte.dev",
                "url": "https://snowbyte.dev/"
            }
        ],
        "links": {
            "repository": "https://github.com/ziehnert/tree-sitter-haproxy"
        }
    },
    "bindings": {
        "c": true,
        "go": true,
        "node": true,
        "python": true,
        "rust": true,
        "swift": true,
        "zig": false
    }
}
